{
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json": "^2.0.20",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.0",
    "@types/koa-send": "^4.1.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.52",
    "@types/plist": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^3.0.1",
    "standard": "^14.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^5.1.6"
  },
  "private": "true",
  "name": "grapefruit-server",
  "main": "dist/app.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "postinstall": "npx ts-node scripts/migrate",
    "build": "tsc",
    "dev": "nodemon",
    "lint": "eslint app.ts lib/**.ts"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "lib",
      "app.ts"
    ],
    "exec": "npx cross-env NODE_ENV=development node -r ts-node/register app.ts",
    "ext": "ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "iOS",
    "mobile",
    "security",
    "instruments",
    "re"
  ],
  "author": "CodeColorist",
  "license": "MIT"
}
