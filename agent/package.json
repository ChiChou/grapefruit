{
  "name": "grapefruit-agent",
  "description": "",
  "private": true,
  "main": "dist.js",
  "scripts": {
    "prepare": "npm run preprocessor && npm run build",
    "build": "frida-compile src/index.ts -o dist.js",
    "watch": "npm run build -- -w",
    "preprocessor": "node scripts/preprocessor.js",
    "types": "tsc --declaration --emitDeclarationOnly --outDir types src/modules/* --target es2019 --skipLibCheck"
  },
  "files": [
    "dist.js",
    "eval.js"
  ],
  "type": "module",
  "devDependencies": {
    "@types/frida-gum": "^18.4.1",
    "@types/node": "^16.3.1",
    "frida-compile": "^16.3.0",
    "typescript": "^5.5.2"
  }
}
